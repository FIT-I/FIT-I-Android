<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageButton
        android:id="@+id/ib_back_arrow"
        android:layout_width="12dp"
        android:layout_height="20dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="70dp"
        android:backgroundTint="#FFFFFF"
        android:padding="0sp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_back"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    <TextView
        android:id="@+id/tv_check_matching"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="매칭요청 확인"
        android:textColor="@color/main"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <ImageView
        android:id="@+id/iv_background_step"
        android:layout_width="0dp"
        android:layout_height="5dp"
        android:layout_marginTop="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_bar" />

    <TextView

        android:id="@+id/tv_service_request"
        android:layout_width="348dp"
        android:layout_height="250dp"
        android:layout_marginTop="22dp"
        android:background="#F8F8F8"
        android:gravity="center_vertical|top"
        android:paddingLeft="47dp"
        android:paddingTop="37dp"
        android:text="서비스 종류"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_background_step" />

    <ImageView
        android:id="@+id/iv_match_bar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="63dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_service_request"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toTopOf="@+id/tv_service_request"
        app:srcCompat="@drawable/line" />

    <ImageView
        android:id="@+id/iv_match_bar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_service_request"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar"
        app:srcCompat="@drawable/line" />

    <ImageView
        android:id="@+id/iv_match_bar3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_service_request"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar2"
        app:srcCompat="@drawable/line" />

    <TextView
        android:id="@+id/tv_price_per_hour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="시간 당"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar2"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar" />

    <TextView
        android:id="@+id/tv_match_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="매칭 날짜"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar3"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar2" />

    <TextView
        android:id="@+id/tv_pickup_way"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:text="픽업 형태"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar3" />

    <TextView
        android:id="@+id/tv_pickup_way2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="149dp"
        android:layout_marginTop="20dp"
        android:text="제가 직접 갈게요"
        android:textColor="@color/main"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tv_pickup_way"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar3" />

    <TextView
        android:id="@+id/tv_match_date2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="23.1.18~23.1.25 "
        android:textColor="@color/main"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar3"
        app:layout_constraintStart_toEndOf="@+id/tv_match_date"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tv_match_date3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="총8일"
        android:textStyle="bold"
        android:textColor="@color/main"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar3"
        app:layout_constraintStart_toEndOf="@+id/tv_match_date2"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tv_total_price2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="208dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="20,000"
        android:textColor="@color/main"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar2"
        app:layout_constraintStart_toEndOf="@+id/tv_price_per_hour"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar" />

    <TextView
        android:id="@+id/tv_total_price3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="원"
        android:textColor="@color/main"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar2"
        app:layout_constraintStart_toEndOf="@+id/tv_total_price2"
        app:layout_constraintTop_toBottomOf="@+id/iv_match_bar" />

    <ImageView
        android:id="@+id/iv_match_service_icon"
        android:layout_width="18dp"
        android:layout_height="23dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="320dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_match_bar"
        app:layout_constraintEnd_toEndOf="@+id/tv_service_request"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_service_request"
        app:layout_constraintTop_toTopOf="@+id/tv_service_request"
        app:srcCompat="@drawable/ic_match_request_check" />
    <Button
        android:id="@+id/btn_next"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_margin="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="@color/main"
        android:text="요청하기"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>