<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff">

        <ImageView
            android:id="@+id/certificate1"
            android:layout_width="95dp"
            android:layout_height="20dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="292dp"
            android:layout_marginBottom="521dp"
            app:srcCompat="@drawable/img_certificate_background"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="189dp" />

        <TextView
            android:id="@+id/certificate_context1"
            android:layout_width="73dp"
            android:layout_height="11dp"
            android:layout_alignLeft="@+id/certificate1"
            android:layout_alignTop="@+id/certificate1"
            android:layout_alignRight="@+id/certificate1"
            android:layout_alignBottom="@+id/certificate1"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginBottom="5dp"
            android:text="NCSA 자격증 보유"
            android:textSize="9sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/review"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="19dp"
        android:text="리뷰"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.86" />

    <TextView
        android:id="@+id/about_me"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="20dp"
        android:text="소개 글"
        app:layout_constraintBottom_toTopOf="@+id/review"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/match_request_btn"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/round_button"
        android:text="@string/match_request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintTop_toBottomOf="@+id/review"
        app:layout_constraintVertical_bias="0.636"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/reviewBtn"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="180dp"
        android:text="@string/see_more_review"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/review" />

    <ImageButton
        android:id="@+id/heart_btn"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/match_request_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/review"
        app:srcCompat="@drawable/button_heart_drawable"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068"
        app:srcCompat="@drawable/img_trainerprofile" />

    <TextView
        android:id="@+id/trainer_name"
        android:layout_width="41dp"
        android:layout_height="20dp"
        android:text="김동현"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.185" />

    <ImageView
        android:id="@+id/trainer_grade"
        android:layout_width="51dp"
        android:layout_height="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toEndOf="@+id/trainer_name"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.184"
        app:srcCompat="@drawable/img_gold" />

    <TextView
        android:id="@+id/distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:text="1km"
        app:layout_constraintEnd_toStartOf="@+id/dot4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="156dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="10dp"
        android:layout_height="11dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:srcCompat="@drawable/ic_star"
        tools:layout_editor_absoluteY="159dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="4.3"
        app:layout_constraintEnd_toStartOf="@+id/dot"
        tools:layout_editor_absoluteY="156dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="93dp"
        android:layout_height="17dp"
        android:layout_marginEnd="8dp"
        android:autoText="false"
        android:text="보유 자격증 5개"
        app:layout_constraintEnd_toStartOf="@+id/dot5"
        tools:layout_editor_absoluteY="156dp" />

    <ImageView
        android:id="@+id/dot"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:srcCompat="@drawable/ic_dot"
        tools:layout_editor_absoluteY="162dp" />

    <ImageView
        android:id="@+id/dot5"
        android:layout_width="3dp"
        android:layout_height="3dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:srcCompat="@drawable/ic_dot"
        tools:layout_editor_absoluteY="159dp" />

    <ImageView
        android:id="@+id/dot4"
        android:layout_width="3dp"
        android:layout_height="3dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:srcCompat="@drawable/ic_dot"
        tools:layout_editor_absoluteY="167dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:text="숭실대학교 재학"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="156dp" />


    <ImageView
        android:id="@+id/certificate2"
        android:layout_width="95dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/img_certificate_background"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="189dp" />

    <ImageView
        android:id="@+id/certificate3"
        android:layout_width="95dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/img_certificate_background"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="189dp" />

    <ImageView
        android:id="@+id/certificate4"
        android:layout_width="95dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/img_certificate_background"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="216dp" />

    <ImageView
        android:id="@+id/certificate5"
        android:layout_width="95dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/img_certificate_background"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="216dp" />

</androidx.constraintlayout.widget.ConstraintLayout>