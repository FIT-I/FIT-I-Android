<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/ib_pre6"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_margin="50dp"
        android:layout_marginStart="10dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@color/main"
        android:scaleType="fitStart"
        android:src="@drawable/ic_pre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_review_ing"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/ib_pre6"
        app:layout_constraintBottom_toBottomOf="@+id/ib_pre6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="매칭 후기 남기기"
        android:textSize="20sp"
        android:textColor="@color/main"
        android:textStyle="bold"/>
    <ImageView
        android:id="@+id/iv_line4"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#F8F8F8"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_review_ing"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        app:layout_constraintTop_toBottomOf="@+id/iv_line4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:circularflow_radiusInDP="20dp"
        android:padding="10dp"
        android:background="@drawable/layout_background">
        <TextView
            android:id="@+id/tv_star"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:padding="5dp"
            android:text="별점"
            android:textColor="@color/main"/>

        <ImageButton
            android:id="@+id/iv_s1"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_margin="5dp"
            android:scaleType="fitCenter"
            android:background="transparent"
            android:src="@drawable/star_review"
            app:layout_constraintTop_toTopOf="@+id/tv_star"
            app:layout_constraintBottom_toBottomOf="@+id/tv_star"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageButton
            android:id="@+id/iv_s2"
            android:layout_margin="5dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:scaleType="fitCenter"
            android:background="transparent"
            android:src="@drawable/star_review"
            app:layout_constraintTop_toTopOf="@+id/tv_star"
            app:layout_constraintBottom_toBottomOf="@+id/tv_star"
            app:layout_constraintEnd_toStartOf="@+id/iv_s1"/>
        <ImageButton
            android:id="@+id/iv_s3"
            android:layout_margin="5dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:scaleType="fitCenter"
            android:background="transparent"
            android:src="@drawable/star_review"
            app:layout_constraintTop_toTopOf="@+id/tv_star"
            app:layout_constraintBottom_toBottomOf="@+id/tv_star"
            app:layout_constraintEnd_toStartOf="@+id/iv_s2"/>
        <ImageButton
            android:id="@+id/iv_s4"
            android:layout_margin="5dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:scaleType="fitCenter"
            android:background="transparent"
            android:src="@drawable/star_review"
            app:layout_constraintTop_toTopOf="@+id/tv_star"
            app:layout_constraintBottom_toBottomOf="@+id/tv_star"
            app:layout_constraintEnd_toStartOf="@+id/iv_s3"/>
        <ImageButton
            android:id="@+id/iv_s5"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_margin="5dp"
            android:scaleType="fitCenter"
            android:background="transparent"
            android:src="@drawable/star_review"
            app:layout_constraintTop_toTopOf="@+id/tv_star"
            app:layout_constraintBottom_toBottomOf="@+id/tv_star"
            app:layout_constraintEnd_toStartOf="@+id/iv_s4"/>
        <ImageView
            android:id="@+id/iv_line5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D9D9D9"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_star"/>
        <TextView
            android:id="@+id/tv_review_match"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="매칭후기"
            android:padding="5dp"
            android:textSize="15sp"
            android:textColor="@color/main"
            app:layout_constraintTop_toBottomOf="@+id/iv_line5"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="parent" />
        <ImageView
            android:id="@+id/iv_line6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D9D9D9"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_review_match"/>
        <EditText
            android:id="@+id/et_review"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main"
            android:textSize="15sp"
            android:layout_marginTop="5dp"
            android:paddingTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/iv_line6"
            app:layout_constraintStart_toStartOf="parent"
            android:background="transparent"/>


    </androidx.constraintlayout.widget.ConstraintLayout>



  </androidx.constraintlayout.widget.ConstraintLayout>